<%= simple_form_for @vehicle do |f| %>
  <%= f.input :name %>
  <%= f.association :type %>
  <%= f.input :price, hint: 'Price should be in euros per day' %>
  <%= f.input :description %>
  <%= f.input :seats %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :address_street %>
  <%= f.input :address_city %>
  <%= f.input :address_zipcode %>
  <%= f.button :submit %>
<% end %>
